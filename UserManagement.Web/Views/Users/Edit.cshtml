@model UserManagement.Web.Models.Users.UpdateUserModel


<h1>Edit User</h1>

<form method="post"
      asp-controller="Users"
      asp-action="Edit">

    <input type="hidden" asp-for="Id" />

    @await Html.PartialAsync("_UserForm", Model)

    <br/>
    <button type="submit" class="btn btn-primary">Update</button>
</form>
